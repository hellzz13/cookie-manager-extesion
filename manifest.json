{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Cookie Manager",
    "description": "Extensão Cookie Manager",
    "author": "Hélio Palença Neto",
    "permissions": [
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_title": "Cookie Manager",
        "default_popup": "popup.html",
        "default_icon": "favicon.png"
    }
}